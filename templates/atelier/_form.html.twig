
<section id="inscriptionA" class="container-fluid">

{{ form_start(form) }}
    {# {{ form_widget(form) }} #}
    <div class="form-group">
    {{form_label(form.NomCarrosserie, 'Nom de la carrosserie')}}
    {{form_widget(form.NomCarrosserie, {'attr':{'class':'form-control',
                                             'placeholder':'Nom de la carroserie'} })}}
    </div>
    <div class="form-group">
    {{ form_label(form.EmailAdresse, 'Adresse e-mail')}}
    {{form_widget(form.EmailAdresse, {'attr':{'class':'form-control', 'placeholder':'@Email'}})}}
    </div>
    <div class="form-group">
    {{form_label(form.AdresseAtelier, "Adresse de l'atelier")}}
    {{form_widget(form.AdresseAtelier, {'attr':{'class':'form-control', 'placeholder':"Adresse de l'Atelier"}})}}
    </div>
    <div class="form-group">
    {{form_row(form.ville, {'attr':{'class':'form-control', 'placeholder':"Adresse de l'Atelier"}})}}
    </div>
     <div class="form-group">
    {{form_row(form.clients, {'attr':{'class':'form-control', 'placeholder':"Adresse de l'Atelier"}})}}
    </div>
    <div class="form-group">
    {{form_row(form.contrats, {'attr':{'class':'form-control', 'placeholder':"Adresse de l'Atelier"}})}}
    </div>
    <div class="form-check">
   {{form_widget(form.attesterCQS,{'attr':{'class':'form-check-input',}})}}
   {{form_label(form.attesterCQS, 'Atelier sous attestation CQS')}}
    </div>
    <div class="form-check">
   {{form_widget(form.certifier,{'attr':{'class':'form-check-input' }})}}
   {{form_label(form.certifier, 'Atelier certifi√©s CQS')}}
    </div>
    
    <button class="btn  btn-success">{{ button_label|default('Sauvegarder') }}</button>
{{ form_end(form) }}
</section>
